{
  "version": 2,
  "rewrites": [
    {
      "source": "/login",
      "destination": "api/index.py"
    },
    {
      "source": "/register",
      "destination": "api/index.py"
    },
    {
      "source": "/sync",
      "destination": "api/index.py"
    },
    {
      "source": "/health",
      "destination": "api/index.py"
    },
    {
      "source": "/cron/(.*)",
      "destination": "api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
